  #!/bin/bash
  PaketInstall() {
  if dpkg -l $1 2> /dev/null
  then
      echo  $1" ist bereits Installiert" 
  else
      if apt-get install $1 -qq -y > /dev/null
      then 
              echo $1 "wurde erfolgreich Installiert"
      else
              echo $1 "konnte nicht installiert werden"
      fi 
 fi
 }
